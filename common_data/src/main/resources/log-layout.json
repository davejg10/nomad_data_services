{
    "level": {
      "$resolver": "level",
      "field": "name"
    },
    "loggerName": {
      "$resolver": "logger",
      "field": "name"
    },
    "message": {
      "$resolver": "message",
      "stringified": true
    },
    "contextMap": {
      "$resolver": "mdc"
    },
    "exception": {
        "$resolver": "exception",
        "field": "stackTrace",
        "stringified": true
    }
  }