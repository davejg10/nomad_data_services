{
  "version": "2.0",
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[4.*, 5.2.0)"
  },
  "logging": {
    "logLevel": {
      "Function": "Warning",
      "Host.Results": "Warning",
      "Host.Aggregator": "Warning",
      "Function.processedQueueConsumer.Console": "Information"
    },
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "maxTelemetryItemsPerSecond" : 4
      },
      "enableLiveMetricsFilters": true
    }
  }
}